{
  "name": "browser-context-plugin",
  "version": "1.0.0",
  "description": "跨浏览器网页内容抓取和解析插件",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:chrome && npm run build:edge && npm run build:safari && npm run build:firefox",
    "build:chrome": "webpack --config webpack.chrome.config.js",
    "build:edge": "webpack --config webpack.edge.config.js", 
    "build:safari": "webpack --config webpack.safari.config.js",
    "build:firefox": "webpack --config webpack.firefox.config.js",
    "dev": "webpack --mode development --watch",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "keywords": [
    "browser-extension",
    "web-scraping",
    "memory-storage",
    "vector-database"
  ],
  "author": "Browser Context Team",
  "license": "MIT",
  "devDependencies": {
    "@types/chrome": "^0.0.251",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.9",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0"
  }
}
