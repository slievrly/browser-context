<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleDisplayName</key>
    <string>Browser Context Plugin</string>
    <key>CFBundleIdentifier</key>
    <string>com.browsercontext.plugin</string>
    <key>CFBundleName</key>
    <string>Browser Context Plugin</string>
    <key>CFBundleShortVersionString</key>
    <string>1.0.0</string>
    <key>CFBundleVersion</key>
    <string>1.0.0</string>
    <key>Author</key>
    <string>Browser Context Team</string>
    <key>Description</key>
    <string>跨浏览器网页内容抓取和解析插件</string>
    <key>ExtensionInfoDictionaryVersion</key>
    <string>1.0</string>
    <key>CFBundlePackageType</key>
    <string>BNDL</string>
    <key>WebKit</key>
    <dict>
        <key>Content</key>
        <dict>
            <key>Scripts</key>
            <array>
                <string>content-script.js</string>
            </array>
            <key>Whitelist</key>
            <array>
                <string>http://*/*</string>
                <string>https://*/*</string>
            </array>
        </dict>
        <key>Background</key>
        <dict>
            <key>Scripts</key>
            <array>
                <string>background.js</string>
            </array>
        </dict>
        <key>Permissions</key>
        <dict>
            <key>Storage</key>
            <true/>
            <key>Tabs</key>
            <true/>
            <key>ActiveTab</key>
            <true/>
        </dict>
    </dict>
    <key>ToolbarItems</key>
    <array>
        <dict>
            <key>DefaultIcon</key>
            <string>icons/icon32.png</string>
            <key>DefaultLabel</key>
            <string>Browser Context Plugin</string>
            <key>Identifier</key>
            <string>com.browsercontext.plugin.toolbar</string>
            <key>Tooltip</key>
            <string>Browser Context Plugin</string>
        </dict>
    </array>
    <key>GlobalPage</key>
    <string>popup.html</string>
</dict>
</plist>
